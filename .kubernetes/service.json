{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "api-docs-service",
    "namespace": "platform",
    "labels": {
      "app": "api-docs-service"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "8000",
        "port": 8000,
        "protocol": "TCP",
        "targetPort": 8000
      }
    ],
    "selector": {
      "app": "api-docs"
    },
    "type": "ClusterIP"
  }
}
